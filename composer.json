{
    "name": "your-vendor/laravel-query-cache",
    "description": "Smart SQL query caching for Laravel with tag-based invalidation",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "YourVendor\\LaravelQueryCache\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^7.3|^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "greenlion/php-sql-parser": "^4.5"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourVendor\\LaravelQueryCache\\QueryCacheServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}